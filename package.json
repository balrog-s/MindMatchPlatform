{
  "name": "platform-api",
  "version": "1.0.0",
  "description": "A back end platform for a blog system",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "build": "babel -d ./build ./src -s --ignore ./node_modules",
    "start-dev": "npm run build && npm run start",
    "start-local": "npm run build && nodemon --watch src --exec npm run start-dev",
    "latest": "knex --knexfile ./knexfile.js migrate:latest",
    "rollback": "knex --knexfile ./knexfile.js migrate:rollback",
    "latest-dev": "knex --knexfile ./knexfile.js migrate:latest",
    "rollback-dev": "knex --knexfile ./knexfile.js migrate:rollback"
  },
  "author": "Balraj Shah",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "^14.5.8",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.2",
    "password-hash": "^1.2.2",
    "pg": "^7.14.0",
    "ramda": "^0.26.1",
    "uuid": "^3.3.3"
  }
}